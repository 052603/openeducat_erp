<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_company_form_inherit" model="ir.ui.view">
			<field name="name">res.company.form.openeducate</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form" />
			<field name="type">form</field>
			<field name="arch" type="xml">
				<xpath expr="/form/group/group/field[@name='logo']" position="after">
					<newline />
					<separator string="Director Signature" />
					<newline />
					<field name="signature" nolabel="1" widget="image" />
				</xpath>
			</field>
		</record>
	</data>
</openerp>